{
  "name": "froggy",
  "publisher": "badlvckinc",
  "displayName": "Froggy",
  "description": "An extension for quick document navigation and manipulation.",
  "version": "3.2.0",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./extension.js",
  "contributes": {
    "keybindings": [
      {
        "key": "ctrl+up",
        "command": "badlvckinc.Jump",
        "args": {
          "leapDistance": 5,
          "direction": "up"
        },
        "when": "editorTextFocus"
      },
      {
        "key": "ctrl+alt+up",
        "command": "badlvckinc.Jump",
        "args": {
          "leapDistance": 10,
          "direction": "up"
        },
        "when": "editorTextFocus"
      },
      {
        "key": "ctrl+down",
        "command": "badlvckinc.Jump",
        "args": {
          "leapDistance": 5,
          "direction": "down"
        },
        "when": "editorTextFocus"
      },
      {
        "key": "ctrl+alt+down",
        "command": "badlvckinc.Jump",
        "args": {
          "leapDistance": 10,
          "direction": "down"
        },
        "when": "editorTextFocus"
      },
      {
        "command": "badlvckinc.GoToLineStart",
        "key": "ctrl+alt+left",
        "when": "editorTextFocus"
      },
      {
        "command": "badlvckinc.GoToLineEnd",
        "key": "ctrl+alt+right",
        "when": "editorTextFocus"
      },
      {
        "key": "ctrl+numpad1",
        "command": "badlvckinc.Skip",
        "args": {
          "skipCount": 1,
          "direction": "left"
        },
        "when": "editorTextFocus"
      },
      {
        "key": "ctrl+alt+numpad1",
        "command": "badlvckinc.Skip",
        "args": {
          "skipCount": 2,
          "direction": "left"
        },
        "when": "editorTextFocus"
      },
      {
        "key": "ctrl+numpad3",
        "command": "badlvckinc.Skip",
        "args": {
          "skipCount": 1,
          "direction": "right"
        },
        "when": "editorTextFocus"
      },
      {
        "key": "ctrl+alt+numpad3",
        "command": "badlvckinc.Skip",
        "args": {
          "skipCount": 2,
          "direction": "right"
        },
        "when": "editorTextFocus"
      },
      {
        "key": "shift+up",
        "command": "badlvckinc.Select",
        "args": {
          "leapDistance": 1,
          "direction": "up"
        },
        "when": "editorTextFocus"
      },
      {
        "key": "shift+down",
        "command": "badlvckinc.Select",
        "args": {
          "leapDistance": 1,
          "direction": "down"
        },
        "when": "editorTextFocus"
      },
      {
        "key": "ctrl+shift+up",
        "command": "badlvckinc.Select",
        "args": {
          "leapDistance": 5,
          "direction": "up"
        },
        "when": "editorTextFocus"
      },
      {
        "key": "ctrl+shift+down",
        "command": "badlvckinc.Select",
        "args": {
          "leapDistance": 5,
          "direction": "down"
        },
        "when": "editorTextFocus"
      },
      {
        "key": "ctrl+alt+shift+up",
        "command": "badlvckinc.Select",
        "args": {
          "leapDistance": 10,
          "direction": "up"
        },
        "when": "editorTextFocus"
      },
      {
        "key": "ctrl+alt+shift+down",
        "command": "badlvckinc.Select",
        "args": {
          "leapDistance": 10,
          "direction": "down"
        },
        "when": "editorTextFocus"
      },
      {
        "key": "ctrl+alt+shift+left",
        "command": "badlvckinc.ExpandSelectionToLineStart",
        "when": "editorTextFocus"
      },
      {
        "key": "ctrl+alt+shift+right",
        "command": "badlvckinc.ExpandSelectionToLineEnd",
        "when": "editorTextFocus"
      }
    ],
    "configuration": [
      {
        "title": "froggy",
        "properties": {}
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.96.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.16.0"
  },
  "dependencies": {
    "node-global-key-listener": "^0.3.0"
  }
}
